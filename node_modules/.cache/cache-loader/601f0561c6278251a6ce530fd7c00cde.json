n  export const frontMatter = {\n\tid: 'doc6',\n\ttitle: 'Build Your Own CAO',\n\tsidebar_label: 'Build Your Own CAO'\n};\nexport const metadata = {\n  \"id\": \"doc6\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Build Your Own CAO\",\n  \"description\": \"Overview\",\n  \"source\": \"@site/docs/doc6.md\",\n  \"permalink\": \"/crypto-comix/docs/doc6\",\n  \"editUrl\": \"https://github.com/burrrata/crypto-comix/edit/master/website/docs/doc6.md\",\n  \"sidebar_label\": \"Build Your Own CAO\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"Open Questions\",\n    \"permalink\": \"/crypto-comix/docs/doc5\"\n  }\n};\n  /* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'Overview',\n  id: 'overview',\n  children: []\n}, {\n  value: 'Prerequisites',\n  id: 'prerequisites',\n  children: []\n}, {\n  value: 'DAO',\n  id: 'dao',\n  children: []\n}, {\n  value: 'Cooperative',\n  id: 'cooperative',\n  children: []\n}];\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component '\" + name + \"' was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"overview\"\n    }}>{`Overview`}</h2>\n    <p>{`This page will walk you through how to deploy your own Cooperative Autonomous Organization.`}</p>\n    <h2 {...{\n      \"id\": \"prerequisites\"\n    }}>{`Prerequisites`}</h2>\n    <ul>\n      <li parentName=\"ul\">{`An Ethereum wallet.`}</li>\n      <li parentName=\"ul\">{`At least 0.1 `}<inlineCode parentName=\"li\">{`ETH`}</inlineCode>{` (likely 0.3 or more).`}</li>\n      <li parentName=\"ul\">{`At least 25 `}<inlineCode parentName=\"li\">{`DAI`}</inlineCode>{` (likely more).`}</li>\n      <li parentName=\"ul\">{`A few hours of time.`}</li>\n      <li parentName=\"ul\">{`A name and mission statement for your CAO.`}</li>\n    </ul>\n    <h2 {...{\n      \"id\": \"dao\"\n    }}>{`DAO`}</h2>\n    <p>{`First go to the `}<a parentName=\"p\" {...{\n        \"href\": \"https://mainnet.aragon.org/#/create\"\n      }}>{`Aragon DAO creation page`}</a>{` and select the Membership template.`}</p>\n    <p>{`Choose your DAO's name as well as voting parameters. For voting we recommend 10% quorum (how many people have to show up to vote) and a 60% approval threshold (of the people who show up, how many have to vote yes for a proposal to pass). Then deploy your DAO (at the time of writing this costs between 0.1 and 0.3 `}<inlineCode parentName=\"p\">{`ETH`}</inlineCode>{` depending on the gas fees).`}</p>\n    <h2 {...{\n      \"id\": \"cooperative\"\n    }}>{`Cooperative`}</h2>\n    <p>{`The first step is to create the `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/burrrata/crypto-comix/blob/master/juris/output.md\"\n      }}>{`articles of incorporation`}</a>{` for your cooperative. Open `}<a parentName=\"p\" {...{\n        \"href\": \"https://github.com/burrrata/crypto-comix/blob/master/juris/output.md\"\n      }}>{`this file`}</a>{` and copy the text into a text editor that can export PDF documents. Then search and replace everything in {{ }} with things specific to your cooperative:`}</p>\n    <ul>\n      <li parentName=\"ul\">{`{{NAME}} => The name of your organization.`}</li>\n      <li parentName=\"ul\">{`{{PURPOSE}} => The mission statement of your organization.`}</li>\n    </ul>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}